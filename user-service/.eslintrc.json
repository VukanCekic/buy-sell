{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["standard-with-typescript", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": [2, "single", { "avoidEscape": true }],
    "@typescript-eslint/explicit-function-return-type": "off"
  }
}
